os:window.votacao,idJogo:parseInt($("#hdnJogo").val())}),contentType:"application/json; charset=utf-8",dataType:"json",success:function(n){n.d==="ok"?bootbox.alert("O seu voto foi enviado com sucesso!",function(){location.reload()}):bootbox.alert(n.d)},error:function(){bootbox.alert("Ocorreu um erro ao enviar a votação. Por favor tente novamente.")}})}function excluiJogador(n){$(".players-field").find("div[data-id="+n+"]").find(".btnExcluir").trigger("click")}if(window.distanciaFinal=0,window.distanciaTopo=0,window.alturaCabeca=$(".navbar.navbar-default.navbar-fixed-top").outerHeight()+$(".navbar.navbar-default.navbar-fixed-top").prev().height(),$("section#Noticia").length>=1){var alturaNoticia=$(".corpo-noticia").height(),info=$("section#Noticia .info.ver");$(document).ready(function(){alturaNoticia>300?($(".corpo-noticia").css({height:"300px",overflow:"hidden"}),$("section#Noticia .ler-mais").show(),$("section#Noticia .corpo-noticia").css("min-height","300px"),info.height(300)):(info.height(alturaNoticia),$("#mrecInread").length>0&&$("#mrecInread").responsiveAd({screenSize:"desktop, tablet, mobile",refresh:!0,slot:"mrecInread"}),$("#mrecInreadEstrelas").length>0&&$("#mrecInreadEstrelas").responsiveAd({screenSize:"desktop, tablet, mobile",refresh:!0,slot:"mrecInreadEstrelas"}))})}$(".btn-ler-mais").click(function(){$(".corpo-noticia").css({height:"100%",overflow:"visible"});info.height(alturaNoticia);$(".ler-mais").hide();window.distanciaFinal=$(".noticia-contentor").offset().top+$(".noticia-contentor").height()-300;window.distanciaTopo=$("section#Noticia .info .social").offset().top-window.alturaCabeca;distanciaTopo>0&&$(window).scroll(function(){if($(".corpo-noticia").height()>300){var n=$("section#Noticia .info .social"),t=n.css("position")==="fixed";$(this).scrollTop()>distanciaTopo&&!t&&n.css({position:"fixed",top:"80px"});$(this).scrollTop()<distanciaTopo&&t&&n.css({position:"static",top:"80px"});$(this).scrollTop()>window.distanciaFinal&&n.css({position:"abs